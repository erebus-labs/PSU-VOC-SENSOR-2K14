 488   1          CharLCD_1_PORT_DR_REG &= ((uint8)(~CharLCD_1_DATA_MASK));
 489   1      
 490   1          /* Write in data, bring E high*/
 491   1          #if(0u != CharLCD_1_PORT_SHIFT) /* MISRA forbids shift by 0 so need to handle that */
                      CharLCD_1_PORT_DR_REG |= 
                          (CharLCD_1_E | ((uint8)(((uint8) nibble) << CharLCD_1_PORT_SHIFT)));
                  #else
 495   1              CharLCD_1_PORT_DR_REG |= (CharLCD_1_E | nibble);
 496   1          #endif /* (0u != CharLCD_1_PORT_SHIFT) */
