 205   2                  break;
 206   2              case (uint8) 2:
 207   2                  CharLCD_1_WriteControl(CharLCD_1_ROW_2_START + column);
 208   2                  break;
 209   2              case (uint8) 3:
 210   2                  CharLCD_1_WriteControl(CharLCD_1_ROW_3_START + column);
 211   2                  break;
 212   2              default:
 213   2                  /* if default case is hit, invalid row argument was passed.*/
